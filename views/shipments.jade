extends layout

block content
  div.ui.container
    h1= title
    p Completely disintermediate market-driven
    console.log(data)
    div.ui.divider
    a.ui.basic.button(href="/shipment-new") Add New
    table.ui.celled.table
      thead
        tr
          th #
          th Title
          th Shipment ID
          th Action
      tbody
        each row in data
          tr
            td= row.id
            td= row.title
            td= row.shipment_id
            td
              a.ui.button(href="/shipments/#{row.id}") View
