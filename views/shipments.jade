extends layout

block content
  div.ui.container
    h1= title
    p Completely disintermediate market-driven
    console.log(data)
    div.ui.divider
    a.ui.basic.button(href="/shipment-new") Add New
    table.ui.celled.table
      thead
        tr
          th #
          th Partner(s)
          th Client(s)
          th Action
      tbody
        each row in data
          tr
            td= row.id
            td= row.partners
            td= row.clients
            td
              a.ui.button(href="#{row.id}") View
